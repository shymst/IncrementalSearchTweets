platform :ios, '9.0'

target 'CompareTestFramework' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'TwitterKit'
end


plugin 'cocoapods-keys', {
  :project => "CompareTestFramework",
  :keys => [
      "TwitterConsumerKey",
      "TwitterConsumerSecret"
  ]
}
